# Example .env file for local development

# Directories
FO_INPUT_DIR=./input
FO_OUTPUT_DIR=./output
FO_MODELS_CACHE=./models_cache

# Processing options
FO_MODE=cluster
FO_WORKERS=4
FO_BATCH_SIZE=16

# Clustering
FO_CLUSTERING_ALGORITHM=kmeans
FO_N_CLUSTERS=5

# Logging
FO_LOG_LEVEL=INFO
FO_VERBOSE=false

# Model settings
FO_CLIP_MODEL=openai/clip-vit-base-patch32
FO_DEVICE=cuda
